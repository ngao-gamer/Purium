-- 2. NÚT Blox Fruit (old) - Tự động đóng UI
Tabs.Main1:AddButton({
    Title="Purium blox fruit (old)",
    Description="",
    Callback=function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ngao-gamer/Purium/refs/heads/main/frrr%20old"))()
        -- Tự động đóng/ẩn Hub chính
        if Window then Window:ToggleVisibility() end 
    end
})

